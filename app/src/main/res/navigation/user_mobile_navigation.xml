<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.gallery.art.user_ui.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/user_fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.gallery.art.user_ui.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/user_fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.gallery.art.user_ui.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/user_fragment_slideshow" />
    <fragment
        tools:layout="@layout/fragment_manage"
        android:id="@+id/navigation_manage"
        android:name="com.gallery.art.admin_ui.manage.ManageFragment"
        android:label="ManageFragment" />
</navigation>